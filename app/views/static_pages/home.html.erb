<% if signed_in? %>
    <div class="row">
      <aside class="span4">
        <section>
          <%= render 'shared/user_info' %>
        </section>
        <section>
          <%= render 'shared/micropost_form' %>
        </section>
      </aside>
    </div>
    <div class="span8">
      <h3>Micropost Feed</h3>
      <%= render 'shared/feed' %>
    </div>
    </div>
<% else %>
<div class="center hero-unit">
<h1>Electronic Portal </h1>
  <h2>
  This is the home page for the
  <a href="http://www.emf.org/">Electronic Music Foundation</a>
</h2>
  <%= link_to "Sign up now!", signup_path, class: "btn btn-large btn-primary" %>
</div>
<%= link_to image_tag("logo.jpg", alt: "EP"), 'http://a1.s6img.com/cdn/box_003/post_13/473709_1862781_lz.jpg/' %>
<% end %>